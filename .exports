# VIM 4 the win
export EDITOR='vim';

# Lang
export LANG='en_US.UTF-8';
export LC_ALL='en_US.UTF-8';

# oh-my-zsh
export ZSH=$HOME/.oh-my-zsh

# path stuff
export PATH="$PATH:/usr/local/bin"
export PATH="$PATH:/usr/local/sbin"
export PATH="$PATH:$HOME/bin"
export PATH="/opt/homebrew/opt/python@3.9/bin:$PATH" #python3 should be first
export PATH="$HOME/.idea:$PATH"

# AWS cli v2
export AWS_PAGER="" # disable less as default pager...
